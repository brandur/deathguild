= content main
  h1 Death Guild
  p List of playlists:
  {{range .PlaylistYears}}
    h2 {{.Year}}
    ul
      {{range .Playlists}}
        li
          a href="/playlists/{{.FormattedDay}}"
            {{.FormattedDay}}
          |  
          a href={{SpotifyPlaylistLink . }} Spotify playlist
      {{end}}
  {{end}}
