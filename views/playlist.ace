= content main
  p
    a href="/" ‚Üê Playlists
  h1 Death Guild {{.Playlist.FormattedDay}}
  p
    a href={{SpotifyPlaylistLink .Playlist}} Spotify playlist
  p {{PlaylistInfo .Playlist}}
  table
    tr
      th Artist
      th Title
      th Spotify ID
    {{range .Playlist.Songs}}
      tr
        td {{.Artist}}
        td {{.Title}}
        td
          {{if ne .SpotifyID ""}}
            a href={{SpotifySongLink .}} {{.SpotifyID}}
          {{end}}
    {{end}}


